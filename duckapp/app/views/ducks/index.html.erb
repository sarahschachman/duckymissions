<h1>Duck Feed</h1>

<table>
	<thead>
		<tr>
			<th>Duck Name</th>
			<th>Location</th>
			<th>Participant Name</th>
			<th>Blog</th>
		</tr>
	</thead>

	<tbody>
		<% @ducks.each do |duck| %>
			<tr>
				<td><%= duck.name %></td>
			</tr>
		<% end %>
		<!--Can I do post.duck.name below? -->
		<% @posts.each do |post| %>
			<tr>
				<td><%= post.location %></td>
				<td><%= post.name %> | <%= post.instagram %></td>
				<td><%= post.blog %></td>
			</tr>
		<% end %>
	</tbody>
</table>
