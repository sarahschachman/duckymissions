<!DOCTYPE html>
<html>
<head>
  <title>Ducky Missions</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  
</head>
<body>
	<!-- logo and menu -->
	<div id="site-wrapper">
		<div id="site-canvas">
			
			<%= image_tag("full_logo.png", class: "logo") %>
			
			<a href="#" class="toggle-nav btn btn-menu"><%= icon('bars') %></a>
			
			
			<%= yield %>

			<div id="footer">
				<p>#duckymissions &copy 2014</p>
			</div>
			
			<div id="site-menu">
				<h2>Menu</h2>
			</div><!--end menu-->

		</div><!-- end #site-canvas -->
	</div><!-- end #site-wrapper -->

</body>
</html>
